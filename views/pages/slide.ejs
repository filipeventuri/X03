<%- include ("../partials/header.ejs") %>
<%- include ("../partials/navbar.ejs")%>
<div class="container  swiper">
    
    <div class="swiper-wrapper">
     
      <div class="swiper-slide " ><img src="/img/0.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/1.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/2.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/3.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/4.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/5.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/6.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/7.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/8.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/9.jpeg" class="img-fluid" ></div>
      <div class="swiper-slide" ><img src="/img/10.jpeg" class="img-fluid" ></div>

      
    </div>
    
    <div class="swiper-pagination"></div>
  
    
    <div class="swiper-button-prev"></div>
    <div class="swiper-button-next"></div>
  

    <div class="swiper-scrollbar"></div>
  </div>
  
  <%- include ("../partials/footer.ejs") %>
  
  <script>
    const swiper = new Swiper('.swiper', {
    // Optional parameters
    direction: 'horizontal',
    loop: true,
  
    // If we need pagination
    pagination: {
      el: '.swiper-pagination',
    },
  
    // Navigation arrows
    navigation: {
      nextEl: '.swiper-button-next',
      prevEl: '.swiper-button-prev',
    },
  
    // And if we need scrollbar
    scrollbar: {
      el: '.swiper-scrollbar',
    },
  });
  </script>
